<odoo>
	<data noupdate="1">
		<!-- Sequences for books.books -->
		<record id="seq_books_books" model="ir.sequence">
			<field name="name">Books Sequence</field>
			<field name="code">books.seq</field>
			<field name="prefix">BK/%(year)s/%(month)s/</field>
			<field name="number_next">1</field>
			<field name="padding">1</field>
			<!-- <field name="company_id" eval="False"/> -->
		</record>

		<!-- Sequences for issue.books -->
		<record id="seq_issue_books" model="ir.sequence">
			<field name="name">Issue Books Sequence</field>
			<field name="code">issuebooks.seq</field>
			<field name="prefix">IBK/%(year)s/%(month)s/</field>
			<field name="number_next">1</field>
			<field name="padding">1</field>
			<!-- <field name="company_id" eval="False"/> -->
		</record>

		<!-- Sequences for library.memberships -->
		<record id="seq_library_memberships" model="ir.sequence">
			<field name="name">Memberships Sequence</field>
			<field name="code">memberships.seq</field>
			<field name="prefix">MEM/%(year)s/%(month)s/</field>
			<field name="number_next">1</field>
			<field name="padding">1</field>
			<!-- <field name="company_id" eval="False"/> -->
		</record>

		<!-- Sequences for authors.authors -->
		<record id="seq_authors_authors" model="ir.sequence">
			<field name="name">Authors Sequence</field>
			<field name="code">authors.seq</field>
			<field name="prefix">A00</field>
			<field name="number_next">1</field>
			<field name="padding">1</field>
			<!-- <field name="company_id" eval="False"/> -->
		</record>
		
	</data>
</odoo>
