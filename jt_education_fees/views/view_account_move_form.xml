<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="account_invoice_view_form_inherit_studnet_details" model="ir.ui.view">
            <field name="name">account.move.view.form.inherit.studnet.details</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//page[@name='other_info']" position="after">
                        <page name="student_details" string="Student Details">
                            <group>
                                <group>
                                    <field name="standard"/>
                                    <field name="division"/>
                                    <field name="year"/>
                                    <!-- <field name="month"/> -->
                                </group>
                            </group>
                        </page>
                    </xpath>
                </data>
            </field>
        </record>
        <record id="account_invoice_view_tree_inherit_studnet_details" model="ir.ui.view">
            <field name="name">account.move.view.tree.inherit.studnet.details</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_out_invoice_tree"/>
            <field name="arch" type="xml">
                <field name="invoice_date" position="before">
                    <field name="standard"/>
                    <field name="division"/>
                    <field name="year"/>
                    <!-- <field name="month"/> -->
                </field>
            </field>
        </record>
    </data>
</odoo>